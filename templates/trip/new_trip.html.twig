{% extends 'base.html.twig' %}
{% block body %}
    <div class="container row">
        <div class="card">
            {{ form_start(form) }}
                {{ form_widget(form.startedAt) }}
                {{ form_row(form.location) }}
                {{ form_row(form.promoter) }}
            {{ form_end(form) }}
        </div>
    </div>
    <script>
        $(document).ready(function(){
            $('select').formSelect();
            $('.datepicker').datepicker();

        });
    </script>
{% endblock %}